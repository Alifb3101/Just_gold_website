{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "allowJs": true,
    "checkJs": false,
    "strict": true,
    "types": ["vite/client"],
    "outDir": "./dist",
    "rootDir": "./src"
  },
  "include": ["src"]
}
